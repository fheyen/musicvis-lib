{
  "name": "musicvis-lib",
  "version": "0.27.0",
  "description": "Music visualization library",
  "author": "Frank Heyen",
  "license": "ISC",
  "homepage": "https://github.com/fheyen/musicvis-lib",
  "repository": {
    "url": "https://github.com/fheyen/musicvis-lib",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/fheyen/musicvis-lib/issues"
  },
  "keywords": [
    "music visualization",
    "visual analytics",
    "JavaScript",
    "web-based",
    "MIDI",
    "music"
  ],
  "main": "dist/index_bundle.js",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "buildprod": "webpack --config webpack.prod.js",
    "test": "jest",
    "testcov": "jest --coverage",
    "testchanged": "jest --coverage --changedFilesWithAncestor",
    "testclear": "jest --clearCache",
    "predeploy": "jest --silent && webpack --config webpack.dev.js && webpack --config webpack.prod.js",
    "deploy": "npm publish && git add . && git commit -m 'new version build'; git push",
    "checkupdates": "npm-check-updates -u"
  },
  "dependencies": {
    "@tonaljs/tonal": "^4.6.0",
    "d3": "^6.3.1",
    "dynamic-time-warping-2": "^1.1.2",
    "soundfont-player": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^26.0.19",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "npm-check-updates": "^10.2.5",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  }
}
