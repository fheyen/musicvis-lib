{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "src/",
            // "package.json", // Would create new folder for each version
            "README.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        // "template": "./node_modules/minami",
        "template": "./node_modules/docdash/",
        "verbose": true
    }
}
