{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "src/",
            // "package.json", // Would create new folder for each version
            "README.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true,
        "default": {
            "includeDate": false
        }
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./node_modules/clean-jsdoc-theme",
        "verbose": true,
        "theme_opts": {
            "theme": "dark",
            "title": "musicvis-lib",
            "menu": [
                {
                    "title": "Website",
                    "link": "https://fheyen.github.io/musicvis-lib",
                    "target": "_blank"
                },
                {
                    "title": "Github",
                    "link": "https://github.com/fheyen/musicvis-lib",
                    "target": "_blank"
                }
            ]
        }
    }
}
